{
  "name": "pikku-i18n",
  "version": "1.0.2",
  "description": "Tiny i18n library for ES6 and React",
  "author": "Mikko Vänskä <mikkovanska.com@gmail.com>",
  "keywords": [
    "i18n",
    "react",
    "gatsby"
  ],
  "files": [
    "lib/"
  ],
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vanska/pikku-i18n.git"
  },
  "main": "./lib/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2",
    "yalc-watch": "0.0.6"
  },
  "peerDependencies": {
    "react": "^16.8.1"
  },
  "scripts": {
    "yalc-watch": "yalc-watch",
    "test": "jest",
    "test-watch": "npm run test -- --watch",
    "build": "tsc",
    "watch": "tsc --watch --noEmit --project './tsconfig.json'"
  },
  "yalcWatch": {
    "watchFolder": "lib",
    "buildWatchCommand": "echo 'Watching for file changes'",
    "extensions": "js"
  }
}
